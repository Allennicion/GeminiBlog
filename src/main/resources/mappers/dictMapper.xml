<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gemini.geminiblog.Dao.DictCategoryDao">

    <update id="delById">
        update gemini_category a
        set a.is_del= 1
        where a.id = #{id}
    </update>
</mapper>